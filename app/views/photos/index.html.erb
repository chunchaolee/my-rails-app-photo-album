<div class="container">
<table class="table table-striped table-condensed ">
  <tr>
    <th>Name</th> 
    <th>Date</th>
    <th>Details</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
  <% @photos.each do |photo| %>
    <tr>
      <td><%= photo.title %></td>
      <td><%= photo.date %></td>
      <td><%= link_to "<span class='glyphicon glyphicon-open-file'></span>  Show".html_safe, photo_path(photo), class: "btn btn-info" %></td>
      <td><%= link_to "<span class='glyphicon glyphicon-edit'></span>  Edit".html_safe, edit_photo_path(photo), class: "btn btn-default" %></td>
      <td><%= link_to "<span class='glyphicon glyphicon-remove-sign'></span>  Destroy".html_safe, photo_path(photo), class: "btn btn-danger" ,method: :delete, data:{ confirm: "Are you sure?"} %></td>
    </tr>
  <% end %>
</table>

<%=link_to "<span class='glyphicon glyphicon-plus'></span>  New Task".html_safe, new_photo_path, class: "btn btn-primary" %>
</div>
